<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label>Title</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="title"></g:TextBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Icon</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="icon"></g:TextBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Homepage</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="homepage"></g:TextBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Country</g:Label>
				</g:customCell>
				<g:customCell>
					<g:SuggestBox ui:field="country"></g:SuggestBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Hometown</g:Label>
				</g:customCell>
				<g:customCell>
					<g:SuggestBox ui:field="hometown"></g:SuggestBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Description</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextArea ui:field="description"></g:TextArea>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Tags</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextArea ui:field="tags"></g:TextArea>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 