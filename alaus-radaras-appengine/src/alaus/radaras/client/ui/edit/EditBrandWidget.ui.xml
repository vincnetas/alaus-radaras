<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label>Title</g:Label><g:TextBox ui:field="title"></g:TextBox>
		<g:Label>Icon</g:Label><g:TextBox ui:field="icon"></g:TextBox>
		<g:Label>Homepage</g:Label><g:TextBox ui:field="homepage"></g:TextBox>
		<g:Label>Country</g:Label><g:SuggestBox ui:field="country"></g:SuggestBox>
		<g:Label>Hometown</g:Label><g:SuggestBox ui:field="hometown"></g:SuggestBox>
		<g:Label>Description</g:Label><g:TextArea ui:field="description"></g:TextArea>
	</g:HTMLPanel>
</ui:UiBinder> 