<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:alaus.radaras.client.ui.edit.suggest" xmlns:r="urn:import:alaus.radaras.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.title {
			font-weight: bold;
			font-size: larger;
		}
		.label {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label>Title</g:Label><g:TextBox ui:field="title"></g:TextBox>
		<g:Label>Country</g:Label><g:TextBox ui:field="country"></g:TextBox>
		<g:Label>City</g:Label><g:TextBox ui:field="city"></g:TextBox>
		<g:Label>Address</g:Label><g:TextBox ui:field="address"></g:TextBox>
		<g:Label>Location</g:Label><r:SelectLocationWidget ui:field="location"></r:SelectLocationWidget>
		<g:Label>Phone</g:Label><g:TextBox ui:field="phone"></g:TextBox>
		<g:Label>Homepage</g:Label><g:TextBox ui:field="homepage"></g:TextBox>
		<g:VerticalPanel ui:field="beerPanel"></g:VerticalPanel>
		<e:BeerSuggestBox ui:field="beerSuggest"></e:BeerSuggestBox>
	</g:HTMLPanel>
</ui:UiBinder> 