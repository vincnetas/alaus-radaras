<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:alaus.radaras.client.ui.edit">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>

	<g:DialogBox ui:field="editPubDialog" modal="true" autoHide="true">
		<g:caption>Edit beer</g:caption>
		<g:HTMLPanel>
			<e:SaveComposite ui:field="widget"></e:SaveComposite>
			<g:Button ui:field="okButton">Ok</g:Button>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder> 