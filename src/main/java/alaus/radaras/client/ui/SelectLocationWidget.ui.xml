<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label>Long</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="longitude"></g:TextBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label>Lat</g:Label>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="latitude"></g:TextBox>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 