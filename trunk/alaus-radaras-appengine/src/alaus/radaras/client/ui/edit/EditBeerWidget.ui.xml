<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label>Title</g:Label><g:TextBox ui:field="title"></g:TextBox>
		<g:Label>Brand</g:Label><g:TextBox ui:field="brand"></g:TextBox>
		<g:Label>Description</g:Label><g:TextArea ui:field="description"></g:TextArea>
		<g:Label>Tags</g:Label><g:TextArea ui:field="tags"></g:TextArea>
	</g:HTMLPanel>
</ui:UiBinder> 