<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:FormPanel ui:field="formPanel" action="/admin/upload" encoding="multipart/form-data" method="post">
			<g:VerticalPanel>
				<g:FileUpload ui:field="fileUpload" name="file"></g:FileUpload>
			</g:VerticalPanel>				
		</g:FormPanel>
		<g:Button ui:field="submitButton">Submit</g:Button>
		<g:ListBox ui:field="list"></g:ListBox>
	</g:HTMLPanel>
</ui:UiBinder> 