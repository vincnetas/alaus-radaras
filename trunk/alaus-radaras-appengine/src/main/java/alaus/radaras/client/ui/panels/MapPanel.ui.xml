<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:com.google.gwt.maps.client" xmlns:r="urn:import:alaus.radaras.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:AbsolutePanel width="100%" height="100%">
		
		<g:LayoutPanel width="100%" height="100%">
			<g:layer>
				<m:MapWidget ui:field="mapWidget" width="100%" height="100%"></m:MapWidget>			
			</g:layer>
		</g:LayoutPanel>
		
	</g:AbsolutePanel>
</ui:UiBinder> 