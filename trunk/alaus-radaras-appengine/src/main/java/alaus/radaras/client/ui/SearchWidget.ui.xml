<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.important}" height="100%" width="300px">
		<g:TextBox width="100%"></g:TextBox>
		<g:ListBox ui:field="listBox" height="200px" width="100%"></g:ListBox>
	</g:HTMLPanel>
</ui:UiBinder> 